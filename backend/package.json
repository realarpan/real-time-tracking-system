  "description": "Real-time tracking system backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\""
